{
    "version": 2,
    "builds": [
        {
            "src": "app/src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/src/app.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": [
        "cdg1"
    ],
    "github": {
        "silent": true
    }
}